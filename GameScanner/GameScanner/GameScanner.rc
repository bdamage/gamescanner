    BEGIN
        MENUITEM "Documentation && FAQ",        ID_HELP_DOCUMENTATIONFAQ
        MENUITEM "Check for updates",           IDM_UPDATE
        MENUITEM "&About ...",                  IDM_ABOUT
    END
END

