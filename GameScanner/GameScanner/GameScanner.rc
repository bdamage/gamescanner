        MENUITEM "Add From Selected Player List", ID_BUDDY_ADDFROMPLAYERLIST
        MENUITEM "Add New Buddy",               ID_BUDDY_ADD
        MENUITEM "Remove Selected Buddy",       ID_BUDDY_REMOVE
