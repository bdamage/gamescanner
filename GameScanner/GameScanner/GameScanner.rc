        MENUITEM "Buddylist",                   ID_VIEW_BUDDYLIST, CHECKED
        MENUITEM "Map preview",                 ID_VIEW_MAPPREVIEW, CHECKED
        MENUITEM "Tabs",                        ID_VIEW_PLAYERLIST, CHECKED
